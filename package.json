{
  "name": "api-rest-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src --ext .ts --fix",
    "knex": "tsx node_modules/knex/bin/cli.js -h",
    "migrate:make": "tsx node_modules/knex/bin/cli.js migrate:make -x ts --knexfile knexfile.ts",
    "migrate:latest": "tsx node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "tsx node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile.ts",
    "migrate:down": "tsx node_modules/knex/bin/cli.js migrate:down --knexfile knexfile.ts",
    "migrate:list": "tsx node_modules/knex/bin/cli.js migrate:list --knexfile knexfile.ts",
    "build": "tsup src --out-dir build",
    "test": "vitest"
  },
  "keywords": [],
  "author": "Alison",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^24.6.2",
    "@types/supertest": "^6.0.3",
    "eslint": "^8.57.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "zod": "^4.1.11"
  }
}
